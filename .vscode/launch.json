{
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "justMyCode": true
    },
    {
      "name": "Python: Module",
      "type": "python",
      "request": "launch",
      "module": "${command:extension.commandvariable.file.relativeFileDotsNoExtension}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: example.lambda_handler.list_users_handler",
      "type": "python",
      "request": "launch",
      "program": "src/example/lambda_handler.py",
      "args": [
        "list_users_handler",
        "--verbose",
        "--event",
        "{ 'page': 1, 'limit': 20 }"
      ],
      "justMyCode": true
    }
  ]
}
